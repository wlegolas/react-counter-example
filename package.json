{
  "name": "react-counter-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">= 20.9.0",
    "yarn": ">= 1.22.19"
  },
  "scripts": {
    "build": "tsc && vite build",
    "start": "vite",
